
CREATE TABLE item (
  item_id INT NOT NULL,
  item_name VARCHAR(45) NOT NULL,
  price INT NOT NULL,
  description VARCHAR(100) NOT NULL,
  picture_url VARCHAR(45) NOT NULL,
  count INT NULL,
  PRIMARY KEY (item_id)
);
  
  
 drop table item;
 select * from item;
  

 insert into item values(
	1111,'딸기',4500,
	'면역력을 강화시켜 질병에 효과적이다.',
	'img/berry.jpg',0
);

